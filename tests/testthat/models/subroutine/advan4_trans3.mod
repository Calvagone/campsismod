$PROBLEM 2-compartment model
$INPUT
$DATA dataset.csv IGNORE=I
$SUBROUTINE ADVAN4 TRANS3
$PK
 KA = THETA(1) * EXP(ETA(1))
 CL = THETA(2) * EXP(ETA(2))
 V = THETA(3) * EXP(ETA(3))
 Q = THETA(4) * EXP(ETA(4))
 VSS = THETA(5) * EXP(ETA(5))
 S2 = V
$ERROR 
 CP = F
 OBS_CP = CP *(1+EPS(1))
 Y = OBS_CP
$THETA 1     ; KA 
$THETA 5     ; CL 
$THETA 80    ; V
$THETA 4     ; Q
$THETA 100   ; VSS
$OMEGA 0.025 ; KA
$OMEGA 0.025 ; CL
$OMEGA 0.025 ; V
$OMEGA 0.025 ; Q
$OMEGA 0.025 ; VSS
$SIGMA 0.025 ; PROP
