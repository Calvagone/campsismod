$PROBLEM 2-compartment model
$INPUT
$DATA dataset.csv IGNORE=I
$SUBROUTINE ADVAN3 TRANS4
$PK
 CL = THETA(1) * EXP(ETA(1))
 V1 = THETA(2) * EXP(ETA(2))
 V2 = THETA(3) * EXP(ETA(3))
 Q = THETA(4) * EXP(ETA(4))
 S1 = V1
$ERROR 
 CP = F
 OBS_CP = CP *(1+EPS(1))
 Y = OBS_CP
$THETA 5     ; CL 
$THETA 80    ; V1 
$THETA 20    ; V2 
$THETA 4     ; Q
$OMEGA 0.025 ; CL
$OMEGA 0.025 ; V1
$OMEGA 0.025 ; V2
$OMEGA 0.025 ; Q
$SIGMA 0.025 ; PROP
