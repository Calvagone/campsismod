[MAIN]
TVLAG1=THETA_LAG1
TVDUR1=THETA_DUR1
TVKA1=THETA_KA1
TVVC1=THETA_VC1
TVCL1=THETA_CL1
TVPROP_RUV1=THETA_PROP_RUV1

LAG1=TVLAG1 * exp(ETA_LAG1)
DUR1=TVDUR1 * exp(ETA_DUR1)
KA1=TVKA1 * exp(ETA_KA1)
VC1=TVVC1 * exp(ETA_VC1)
CL1=TVCL1 * exp(ETA_CL1)
PROP_RUV1=TVPROP_RUV1

TVLAG2=THETA_LAG2
TVDUR2=THETA_DUR2
TVKA2=THETA_KA2
TVVC2=THETA_VC2
TVCL2=THETA_CL2
TVPROP_RUV2=THETA_PROP_RUV2

LAG2=TVLAG2 * exp(ETA_LAG2)
DUR2=TVDUR2 * exp(ETA_DUR2)
KA2=TVKA2 * exp(ETA_KA2)
VC2=TVVC2 * exp(ETA_VC2)
CL2=TVCL2 * exp(ETA_CL2)
PROP_RUV2=TVPROP_RUV2

[ODE]
d/dt(A_ABS1)=-KA1*A_ABS1
d/dt(A_CENTRAL1)=KA1*A_ABS1 - CL1/VC1*A_CENTRAL1
d/dt(A_ABS2)=-KA2*A_ABS2
d/dt(A_CENTRAL2)=KA2*A_ABS2 - CL2/VC2*A_CENTRAL2

[LAG]
A_ABS1=LAG1
A_ABS2=LAG2

[DURATION]
A_ABS1=DUR1
A_ABS2=DUR2

[ERROR]
CONC1=A_CENTRAL1/VC1
if (CONC1 <= 0.001) CONC1=0.001
IPRED1=log(CONC1)
W1=PROP_RUV1
Y1=IPRED1 + W1*EPS_RUV_FIX1
CONC2=A_CENTRAL2/VC2
if (CONC2 <= 0.001) CONC2=0.001
IPRED2=log(CONC2)
W2=PROP_RUV2
Y2=IPRED2 + W2*EPS_RUV_FIX2

