[MAIN]
TVLAG=THETA_LAG
TVKA=THETA_KA
TVCL=THETA_CL
TVVC=THETA_VC
TVPROP_RUV=THETA_PROP_RUV

LAG=TVLAG * exp(ETA_LAG)
KA=TVKA * exp(ETA_KA)
CL=TVCL * exp(ETA_CL)
VC=TVVC * exp(ETA_VC)
PROP_RUV=TVPROP_RUV

[ODE]
d/dt(A_ABS)=-KA*A_ABS
d/dt(A_CENTRAL)=KA*A_ABS - CL/VC*A_CENTRAL

[LAG]
A_ABS=LAG

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

