[MAIN]
TVVMABS=THETA_VMABS
TVKMABS=THETA_KMABS
TVCL=THETA_CL
TVVC=THETA_VC
TVPROP_RUV=THETA_PROP_RUV

VMABS=TVVMABS
KMABS=TVKMABS
CL=TVCL * exp(ETA_CL)
VC=TVVC * exp(ETA_VC)
PROP_RUV=TVPROP_RUV

[ODE]
MM=VMABS/(KMABS + A_ABS)
d/dt(A_ABS)=-MM*A_ABS
d/dt(A_CENTRAL)=MM*A_ABS - CL/VC*A_CENTRAL

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

