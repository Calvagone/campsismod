[MAIN]
TVBIO=THETA_BIO
TVVMABS=THETA_VMABS
TVKMABS=THETA_KMABS
TVVC=THETA_VC
TVCL=THETA_CL

BIO=TVBIO
VMABS=TVVMABS
KMABS=TVKMABS
VC=TVVC * exp(ETA_VC)
CL=TVCL * exp(ETA_CL)

[ODE]
MM=VMABS/(KMABS + A_ABS)
d/dt(A_ABS)=-MM*A_ABS
d/dt(A_CENTRAL)=MM*A_ABS - CL/VC*A_CENTRAL

[F]
A_ABS=BIO

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
CONC_ERR=CONC*(1 + EPS_PROP_RUV)

