[MAIN]
TVBIO=THETA_BIO
TVKA=THETA_KA
TVGAM=THETA_GAM
TVVC=THETA_VC
TVCL=THETA_CL

BIO=TVBIO
KA=TVKA * exp(ETA_KA)
GAM=TVGAM * exp(ETA_GAM)
VC=TVVC * exp(ETA_VC)
CL=TVCL * exp(ETA_CL)

[ODE]
WF=1-exp(-pow(KA*(t-TDOS),GAM))
d/dt(A_ABS)=-WF*A_ABS
d/dt(A_CENTRAL)=WF*A_ABS - CL/VC*A_CENTRAL

[F]
A_ABS=BIO

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
CONC_ERR=CONC*(1 + EPS_PROP_RUV)

