[MAIN]
TVLAG=THETA_LAG
TVKA=THETA_KA
TVVC=THETA_VC
TVVMAX=THETA_VMAX
TVKM=THETA_KM
TVPROP_RUV=THETA_PROP_RUV

LAG=TVLAG * exp(ETA_LAG)
KA=TVKA * exp(ETA_KA)
VC=TVVC * exp(ETA_VC)
VMAX=TVVMAX * exp(ETA_VMAX)
KM=TVKM * exp(ETA_KM)
PROP_RUV=TVPROP_RUV

[ODE]
d/dt(A_ABS)=-KA*A_ABS
d/dt(A_CENTRAL)=KA*A_ABS - VMAX/VC*A_CENTRAL/(A_CENTRAL/VC+KM)

[LAG]
A_ABS=LAG

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

