[MAIN]
TVKA=THETA_KA
TVDUR=THETA_DUR
TVVMAX=THETA_VMAX
TVVC=THETA_VC
TVKM=THETA_KM
TVPROP_RUV=THETA_PROP_RUV

KA=TVKA * exp(ETA_KA)
DUR=TVDUR * exp(ETA_DUR)
VMAX=TVVMAX * exp(ETA_VMAX)
VC=TVVC * exp(ETA_VC)
KM=TVKM * exp(ETA_KM)
PROP_RUV=TVPROP_RUV

[ODE]
d/dt(A_ABS)=-KA*A_ABS
d/dt(A_CENTRAL)=KA*A_ABS - VMAX/VC*A_CENTRAL/(A_CENTRAL/VC+KM)

[DURATION]
A_ABS=DUR

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

