[MAIN]
TVBIO=THETA_BIO
TVDUR=THETA_DUR
TVKA=THETA_KA
TVVC=THETA_VC
TVVMAX=THETA_VMAX
TVKM=THETA_KM

BIO=TVBIO
DUR=TVDUR * exp(ETA_DUR)
KA=TVKA * exp(ETA_KA)
VC=TVVC * exp(ETA_VC)
VMAX=TVVMAX * exp(ETA_VMAX)
KM=TVKM * exp(ETA_KM)

[ODE]
d/dt(A_ABS)=-KA*A_ABS
d/dt(A_CENTRAL)=KA*A_ABS - VMAX/VC*A_CENTRAL/(A_CENTRAL/VC+KM)

[F]
A_ABS=BIO

[DURATION]
A_ABS=DUR

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
CONC_ERR=CONC*(1 + EPS_PROP_RUV)

