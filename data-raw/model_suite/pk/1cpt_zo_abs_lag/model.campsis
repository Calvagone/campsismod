[MAIN]
TVLAG=THETA_LAG
TVBIO=THETA_BIO
TVDUR=THETA_DUR
TVKA=THETA_KA
TVVC=THETA_VC
TVCL=THETA_CL

LAG=TVLAG * exp(ETA_LAG)
BIO=TVBIO
DUR=TVDUR * exp(ETA_DUR)
KA=TVKA * exp(ETA_KA)
VC=TVVC * exp(ETA_VC)
CL=TVCL * exp(ETA_CL)

[ODE]
d/dt(A_ABS)=-KA*A_ABS
d/dt(A_CENTRAL)=KA*A_ABS - CL/VC*A_CENTRAL

[F]
A_ABS=BIO

[LAG]
A_ABS=LAG

[DURATION]
A_ABS=DUR

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
CONC_ERR=CONC*(1 + EPS_PROP_RUV)

