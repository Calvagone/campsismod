[MAIN]
TVDUR=THETA_DUR
TVVC=THETA_VC
TVVMAX=THETA_VMAX
TVKM=THETA_KM
TVPROP_RUV=THETA_PROP_RUV

DUR=TVDUR * exp(ETA_DUR)
VC=TVVC * exp(ETA_VC)
VMAX=TVVMAX * exp(ETA_VMAX)
KM=TVKM * exp(ETA_KM)
PROP_RUV=TVPROP_RUV

[ODE]
d/dt(A_CENTRAL)=-VMAX/VC*A_CENTRAL/(A_CENTRAL/VC+KM)

[DURATION]
A_CENTRAL=DUR

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

