[MAIN]
TVDUR=THETA_DUR
TVVC=THETA_VC
TVVMAX=THETA_VMAX
TVKM=THETA_KM

DUR=TVDUR * exp(ETA_DUR)
VC=TVVC * exp(ETA_VC)
VMAX=TVVMAX * exp(ETA_VMAX)
KM=TVKM * exp(ETA_KM)

[ODE]
d/dt(A_CENTRAL)=-VMAX/VC*A_CENTRAL/(A_CENTRAL/VC+KM)

[DURATION]
A_CENTRAL=DUR

[ERROR]
CONC=A_CENTRAL/VC
if (CONC <= 0.001) CONC=0.001
CONC_ERR=CONC*(1 + EPS_PROP_RUV)

